{
  "name": "pdf-reader",
  "version": "1.0.0",
  "description": "Visualizador de archivos pdf",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "release": "standard-version && git push --follow-tags --no-verify",
    "pre-commit": "npm run lint && npm run test",
    "pre-push": "npm run release",
    "lint-fix": "eslint . --ext .js --fix",
    "lint-staged": "lint-staged",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "node -c index.js && node -c src/**/*.js && echo 'Build completed successfully'",
    "prepare": "husky"
  },
  "author": "Johan Posso",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@highlight-run/node": "^3.12.19",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.0.0",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "ioredis": "^5.6.1",
    "logrocket": "^10.1.0",
    "multer": "^2.0.1",
    "nodemon": "^3.1.10",
    "pg": "^8.11.3",
    "redis": "^5.6.0",
    "sequelize": "^6.37.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.31.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "supertest": "^7.1.3"
  }
}
